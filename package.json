{
  "name": "PROJECT_NAME",
  "description": "$PROJECT_DESCRIPTION$",
  "keywords": [
    "$PROJECT_KEYWORDS$"
  ],
  "version": "0.1.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "author": "$PROJECT_AUTHOR$",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx . && flow",
    "test": "npm run lint && jest",
    "watch": "jest --watch",
    "build": "npm run build-clean && npm run build-babel && npm run build-flow",
    "build-clean": "rimraf lib",
    "build-babel": "babel src/ -d lib --ignore __tests__",
    "build-flow": "flow-copy-source src lib --ignore __tests__/**"
  },
  "repository": {
    "type": "git",
    "url": "$GIT_URL$"
  },
  "bugs": {
    "url": "$GIT_URL$/issues"
  },
  "homepage": "$GIT_URL$#readme",
  "dependencies": {
    "flow-runtime": "^0.10.0",
    "react": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-plugin-flow-runtime": "^0.10.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.18.0",
    "eslint-config-reactivestack": "^1.0.0",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-react": "^6.10.0",
    "flow-bin": "^0.42.0",
    "flow-copy-source": "^1.1.0",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1"
  }
}
